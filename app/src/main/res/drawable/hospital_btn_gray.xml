<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape>
            <gradient
                android:startColor="#E3E5E9"
                android:endColor="#E3E5E9"
                android:angle="90" />
            <stroke
                android:width="2dp"
                android:color="#04141B" />
            <corners
                android:radius="25dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp"/>
        </shape>
    </item>

</selector>