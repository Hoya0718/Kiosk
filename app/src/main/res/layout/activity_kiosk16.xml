                android:textSize="18sp" />

            <Button
                android:id="@+id/jieut_btn"
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_weight="1"
                android:text="ㅈ"
                android:textSize="18sp" />

            <Button
                android:id="@+id/chieut_btn"
                android:layout_width="match_parent"
