            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:layout_weight="1"
            android:text="ㅅ"
            android:textSize="18sp" />

        <Button
            android:id="@+id/ieung_btn"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:layout_weight="1"
            android:text="ㅇ"
            android:textSize="18sp" />

        <Button
            android:id="@+id/jieut_btn"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:layout_weight="1"
            android:text="ㅈ"
            android:textSize="18sp" />

        <Button
            android:id="@+id/chieut_btn"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:layout_weight="1"
            android:text="ㅊ"
            android:textSize="18sp" />

        <Button
            android:id="@+id/kieuk_btn"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:layout_weight="1"
            android:text="ㅋ"
            android:textSize="18sp" />

        <Button
            android:id="@+id/tieut_btn"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:layout_weight="1"
            android:text="ㅌ"
            android:textSize="18sp" />

        <Button
            android:id="@+id/pieup_btn"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:layout_weight="1"
            android:text="ㅍ"
            android:textSize="18sp" />

        <Button
            android:id="@+id/hieut_btn"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:layout_weight="1"
            android:text="ㅎ"
            android:textSize="18sp" />
    </LinearLayout>


    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="3" />
</LinearLayout>