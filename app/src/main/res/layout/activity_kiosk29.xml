<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Kiosk_29">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.8"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/barcord_2" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="vertical">

        <TextView
            android:id="@+id/accept_info"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="2"
            android:gravity="center"
            android:text="@string/hospital_list_4"
            android:textSize="@dimen/font_size_medium" />

        <TextView
            android:id="@+id/textView49"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.7" />

        <TextView
            android:id="@+id/SSN_2"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.9"
            android:gravity="center"
            android:hint="@string/ssn_2"
            android:maxLength="14"
            android:textSize="@dimen/font_size_medium" />
    </LinearLayout>

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="2">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1">

            <Button
                android:id="@+id/num_1_2"
                android:layout_width="0dp"
                android:layout_height="120dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:gravity="center|center_horizontal"
                android:onClick="put_n"
                android:text="@string/ssn_btn_1"
                android:textSize="@dimen/font_size_medium" />

            <Button
                android:id="@+id/num_2_2"
                android:layout_width="0dp"
                android:layout_height="120dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:gravity="center|center_horizontal"
                android:onClick="put_n"
                android:text="@string/ssn_btn_2"
                android:textSize="@dimen/font_size_medium" />

            <Button
                android:id="@+id/num_3_2"
                android:layout_width="0dp"
                android:layout_height="120dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:gravity="center|center_horizontal"
                android:onClick="put_n"
                android:text="@string/ssn_btn_3"
                android:textSize="@dimen/font_size_medium" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1">

            <Button
                android:id="@+id/num_4_2"
                android:layout_width="0dp"
                android:layout_height="120dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:gravity="center|center_horizontal"
                android:onClick="put_n"
                android:text="@string/ssn_btn_4"
                android:textSize="@dimen/font_size_medium" />

            <Button
                android:id="@+id/num_5_2"
                android:layout_width="0dp"
                android:layout_height="120dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:gravity="center|center_horizontal"
                android:onClick="put_n"
                android:text="@string/ssn_btn_5"
                android:textSize="@dimen/font_size_medium" />

            <Button
                android:id="@+id/num_6_2"
                android:layout_width="0dp"
                android:layout_height="120dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:gravity="center|center_horizontal"
                android:onClick="put_n"
                android:text="@string/ssn_btn_6"
                android:textSize="@dimen/font_size_medium" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1">

            <Button
                android:id="@+id/num_7_2"
                android:layout_width="0dp"
                android:layout_height="120dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:gravity="center|center_horizontal"
                android:onClick="put_n"
                android:text="@string/ssn_btn_7"
                android:textSize="@dimen/font_size_medium" />

            <Button
                android:id="@+id/num_8_2"
                android:layout_width="0dp"
                android:layout_height="120dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:gravity="center|center_horizontal"
                android:onClick="put_n"
                android:text="@string/ssn_btn_8"
                android:textSize="@dimen/font_size_medium" />

            <Button
                android:id="@+id/num_9_2"
                android:layout_width="0dp"
                android:layout_height="120dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:gravity="center|center_horizontal"
                android:onClick="put_n"
                android:text="@string/ssn_btn_9"
                android:textSize="18sp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1">

            <Button
                android:id="@+id/CL_2"
                android:layout_width="0dp"
                android:layout_height="120dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:gravity="center|center_horizontal"
                android:onClick="put_n"
                android:text="@string/ssn_btn_CL"
                android:textSize="@dimen/font_size_medium" />

            <Button
                android:id="@+id/num_0_2"
                android:layout_width="0dp"
                android:layout_height="120dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:gravity="center|center_horizontal"
                android:onClick="put_n"
                android:text="@string/ssn_btn_0"
                android:textSize="@dimen/font_size_medium" />

            <Button
                android:id="@+id/Check_2_2"
                android:layout_width="0dp"
                android:layout_height="120dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:gravity="center|center_horizontal"
                android:onClick="goto_kiosk_29_2"
                android:text="@string/ssn_btn_check"
                android:textSize="@dimen/font_size_medium" />
        </TableRow>
    </TableLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0.3"
        android:orientation="vertical"></LinearLayout>
</LinearLayout>