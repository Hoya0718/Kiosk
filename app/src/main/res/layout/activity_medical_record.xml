<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Hospital_Medical_Record">

    <Button
        android:id="@+id/h_btn_check_2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/hospital_btn_border"
        android:onClick="goto_progress"
        android:text="확인"
        android:textSize="40sp"
        app:layout_constraintStart_toStartOf="@+id/h_btn_9_2"
        app:layout_constraintTop_toTopOf="@+id/h_btn_0_2" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="600dp"
        android:layout_height="100dp"
        android:gravity="center_vertical"
        android:paddingLeft="50dp"
        android:text="목포해양대학교병원"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView87"
        android:layout_width="700dp"
        android:layout_height="150dp"
        android:layout_marginTop="30dp"
        android:gravity="center"
        android:text="진료카드가 없으신 분은\n진료카드번호 또는 주민등록번호를\n입력 후 확인 버튼을 누르세요."
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView28" />

    <TextView
        android:id="@+id/to_txt_ssn1"
        android:layout_width="500dp"
        android:layout_height="80sp"
        android:layout_marginTop="30dp"
        android:background="@drawable/hospital_txt_border"
        android:gravity="center"
        android:maxLength="14"
        android:padding="10dp"
        android:textSize="100sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView87" />

    <TextView
        android:id="@+id/barcode_txt"
        android:layout_width="700dp"
        android:layout_height="220dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/hospital_txt_blue"
        android:gravity="center"
        android:text="빠른 처리를 원하시는 분은\n진료카드/영수증의 바코드를\n아래 바코드 대는 곳에 읽혀주세요./n"
        android:textSize="30sp"
        app:layout_constraintTop_toBottomOf="@+id/h_btn_0_2"
        tools:layout_editor_absoluteX="50dp" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="200dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/barcode_txt"
        app:srcCompat="@drawable/arrow_br" />

    <Button
        android:id="@+id/h_btn_visitor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/hospital_btn_background_blue"
        android:onClick="put_n"
        android:text="처음\n오신 분\n번호표\n발행"
        android:textColor="#0000FF"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="@+id/h_btn_check_2"
        app:layout_constraintEnd_toEndOf="@+id/barcode_txt" />

    <Button
        android:id="@+id/button25"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/homei"
        android:onClick="goto_Hospital_Main"
        app:layout_constraintStart_toEndOf="@+id/textView28"
        app:layout_constraintTop_toTopOf="@+id/textView28" />

    <Button
        android:id="@+id/h_btn_1_2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginEnd="30dp"
        android:background="@drawable/hospital_btn_border"
        android:onClick="put_n"
        android:text="1"
        android:textSize="40sp"
        app:layout_constraintEnd_toStartOf="@+id/h_btn_2_2"
        app:layout_constraintTop_toTopOf="@+id/h_btn_2_2" />

    <Button
        android:id="@+id/h_btn_2_2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/hospital_btn_border"
        android:onClick="put_n"
        android:text="2"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="@+id/to_txt_ssn1"
        app:layout_constraintStart_toStartOf="@+id/to_txt_ssn1"
        app:layout_constraintTop_toBottomOf="@+id/to_txt_ssn1" />

    <Button
        android:id="@+id/h_btn_3_2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="30dp"
        android:background="@drawable/hospital_btn_border"
        android:onClick="put_n"
        android:text="3"
        android:textSize="40sp"
        app:layout_constraintStart_toEndOf="@+id/h_btn_2_2"
        app:layout_constraintTop_toTopOf="@+id/h_btn_2_2" />

    <Button
        android:id="@+id/h_btn_4_2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/hospital_btn_border"
        android:onClick="put_n"
        android:text="4"
        android:textSize="40sp"
        app:layout_constraintStart_toStartOf="@+id/h_btn_1_2"
        app:layout_constraintTop_toBottomOf="@+id/h_btn_1_2" />

    <Button
        android:id="@+id/h_btn_5_2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/hospital_btn_border"
        android:onClick="put_n"
        android:text="5"
        android:textSize="40sp"
        app:layout_constraintStart_toStartOf="@+id/h_btn_2_2"
        app:layout_constraintTop_toTopOf="@+id/h_btn_4_2" />

    <Button
        android:id="@+id/h_btn_6_2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/hospital_btn_border"
        android:onClick="put_n"
        android:text="6"
        android:textSize="40sp"
        app:layout_constraintStart_toStartOf="@+id/h_btn_3_2"
        app:layout_constraintTop_toTopOf="@+id/h_btn_5_2" />

    <Button
        android:id="@+id/h_btn_7_2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/hospital_btn_border"
        android:onClick="put_n"
        android:text="7"
        android:textSize="40sp"
        app:layout_constraintStart_toStartOf="@+id/h_btn_4_2"
        app:layout_constraintTop_toBottomOf="@+id/h_btn_4_2" />

    <Button
        android:id="@+id/h_btn_8_2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/hospital_btn_border"
        android:onClick="put_n"
        android:text="8"
        android:textSize="40sp"
        app:layout_constraintStart_toStartOf="@+id/h_btn_5_2"
        app:layout_constraintTop_toTopOf="@+id/h_btn_7_2" />

    <Button
        android:id="@+id/h_btn_9_2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/hospital_btn_border"
        android:onClick="put_n"
        android:text="9"
        android:textSize="40sp"
        app:layout_constraintStart_toStartOf="@+id/h_btn_6_2"
        app:layout_constraintTop_toTopOf="@+id/h_btn_8_2" />

    <Button
        android:id="@+id/h_btn_0_2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/hospital_btn_border"
        android:text="0"
        android:textSize="40sp"
        app:layout_constraintStart_toStartOf="@+id/h_btn_8_2"
        app:layout_constraintTop_toTopOf="@+id/h_btn_cancel_2"
        android:onClick="put_n" />

    <Button
        android:id="@+id/h_btn_cancel_2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/hospital_btn_border"
        android:onClick="put_n"
        android:text="정정"
        android:textSize="40sp"
        app:layout_constraintStart_toStartOf="@+id/h_btn_7_2"
        app:layout_constraintTop_toBottomOf="@+id/h_btn_7_2" />

</androidx.constraintlayout.widget.ConstraintLayout>