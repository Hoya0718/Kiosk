<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#DFDFDF"
    tools:context=".r_Hospital_Printing">

    <TextView
        android:id="@+id/textView80"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:gravity="center_vertical"
        android:paddingLeft="50dp"
        android:text="목포해양대학교병원"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="600dp"
        android:layout_height="200dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/hospital_txt_gray_tlr"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView80"
        app:srcCompat="@drawable/diagnosis_print" />

    <TextView
        android:id="@+id/h_printing_txt_printing"
        android:layout_width="600dp"
        android:layout_height="150dp"
        android:background="@drawable/hospital_txt_gray_blr"
        android:gravity="center"
        android:text="출력중입니다.\n잠시만 기다려주세요."
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView12" />

    <TextView
        android:id="@+id/h_printing_txt_examination_fee"
        android:layout_width="600dp"
        android:layout_height="100dp"
        android:background="@drawable/hospital_txt_gray_lr"
        android:gravity="center"
        android:text="진찰비 또는 검사비는 시행당일 수납하셔야 합니다."
        android:textColor="#000000"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="@+id/h_printing_txt_printing"
        app:layout_constraintTop_toBottomOf="@+id/h_printing_txt_printing" />

    <TextView
        android:id="@+id/textView86"
        android:layout_width="600dp"
        android:layout_height="100dp"
        android:background="@drawable/hospital_txt_gray_blr"
        android:gravity="center_horizontal"
        android:text="부득이하게 미리 납부(선납)하신 후 미실시한 진료/검사가\n있을 경우, 수납창구에서 환불해드립니다."
        android:textSize="22sp"
        app:layout_constraintStart_toStartOf="@+id/h_printing_txt_examination_fee"
        app:layout_constraintTop_toBottomOf="@+id/h_printing_txt_examination_fee" />

    <TextView
        android:id="@+id/textView89"
        android:layout_width="400dp"
        android:layout_height="100dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/hospital_txt_gray_tl"
        android:gravity="center_vertical"
        android:paddingLeft="30dp"
        android:text="환자이름"
        android:textColor="#000000"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/textView86"
        app:layout_constraintTop_toBottomOf="@+id/textView86" />

    <TextView
        android:id="@+id/textView98"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:background="@drawable/hospital_txt_gray_tr"
        android:gravity="right|center_vertical"
        android:paddingRight="30dp"
        android:text="홍*동"
        android:textColor="#4169E1"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/textView89"
        app:layout_constraintTop_toTopOf="@+id/textView89" />

    <TextView
        android:id="@+id/textView106"
        android:layout_width="600dp"
        android:layout_height="100dp"
        android:background="@drawable/hospital_txt_gray_tlr"
        android:gravity="bottom|center_horizontal"
        android:text="출력현황"
        android:textColor="#000000"
        android:textSize="40sp"
        app:layout_constraintStart_toStartOf="@+id/textView89"
        app:layout_constraintTop_toBottomOf="@+id/textView89" />

    <TextView
        android:id="@+id/textView107"
        android:layout_width="600dp"
        android:layout_height="100dp"
        android:background="@drawable/hospital_txt_gray_blr"
        android:gravity="center"
        android:text="0/1"
        android:textSize="40sp"
        app:layout_constraintStart_toStartOf="@+id/textView106"
        app:layout_constraintTop_toBottomOf="@+id/textView106" />

    <TextView
        android:id="@+id/textView108"
        android:layout_width="600dp"
        android:layout_height="50dp"
        android:background="@drawable/hospital_txt_gray_blr"
        app:layout_constraintStart_toStartOf="@+id/textView107"
        app:layout_constraintTop_toBottomOf="@+id/textView107" />
</androidx.constraintlayout.widget.ConstraintLayout>