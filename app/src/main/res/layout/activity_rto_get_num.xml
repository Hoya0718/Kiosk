<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#66BBFF"
    tools:context=".r_To_Get_Num">

    <TextView
        android:id="@+id/textView128"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:background="#ffffff"
        android:gravity="center_vertical"
        android:paddingLeft="30dp"
        android:text="주민등록번호(등본)"
        android:textSize="30sp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="0dp" />

    <View
        android:id="@+id/view7"
        android:layout_width="720dp"
        android:layout_height="800dp"
        android:layout_marginTop="100dp"
        android:background="#ffffff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView128" />

    <TextView
        android:id="@+id/textView129"
        android:layout_width="720dp"
        android:layout_height="50dp"
        android:layout_marginTop="30dp"
        android:gravity="center_vertical"
        android:paddingLeft="30dp"
        android:text="발급부수를 입력해 주세요"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/view7"
        app:layout_constraintTop_toTopOf="@+id/view7" />

    <TextView
        android:id="@+id/textView130"
        android:layout_width="400dp"
        android:layout_height="100dp"
        android:layout_marginTop="30dp"
        android:gravity="right|center_vertical"
        android:text="0"
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="@+id/view7"
        app:layout_constraintTop_toBottomOf="@+id/textView129" />

    <Button
        android:id="@+id/gnn_9"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/hospital_btn_gray"
        android:onClick="goto_pay_List"
        android:text="9"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/gnn_6"
        app:layout_constraintTop_toTopOf="@+id/gnn_8" />

    <Button
        android:id="@+id/gnn_8"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/hospital_btn_gray"
        android:onClick="goto_pay_List"
        android:text="8"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/gnn_5"
        app:layout_constraintTop_toTopOf="@+id/gnn_7" />

    <Button
        android:id="@+id/gnn_4"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/hospital_btn_gray"
        android:onClick="goto_pay_List"
        android:text="4"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/gnn_1"
        app:layout_constraintTop_toBottomOf="@+id/gnn_1" />

    <Button
        android:id="@+id/gnn_7"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/hospital_btn_gray"
        android:onClick="goto_pay_List"
        android:text="7"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/gnn_4"
        app:layout_constraintTop_toBottomOf="@+id/gnn_4" />

    <Button
        android:id="@+id/gnn_5"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/hospital_btn_gray"
        android:onClick="goto_pay_List"
        android:text="5"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/gnn_2"
        app:layout_constraintTop_toTopOf="@+id/gnn_4" />

    <Button
        android:id="@+id/gnn_6"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/hospital_btn_gray"
        android:onClick="goto_pay_List"
        android:text="6"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/gnn_3"
        app:layout_constraintTop_toTopOf="@+id/gnn_5" />

    <Button
        android:id="@+id/gnn_3"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="30dp"
        android:background="@drawable/hospital_btn_gray"
        android:onClick="goto_pay_List"
        android:text="3"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/gnn_2"
        app:layout_constraintTop_toTopOf="@+id/gnn_2" />

    <Button
        android:id="@+id/gnn_2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="30dp"
        android:background="@drawable/hospital_btn_gray"
        android:text="2"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/gnn_1"
        app:layout_constraintTop_toTopOf="@+id/gnn_1"
        android:onClick="goto_pay_List" />

    <Button
        android:id="@+id/gnn_1"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="180dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/hospital_btn_gray"
        android:onClick="goto_pay_List"
        android:text="1"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/view7"
        app:layout_constraintTop_toBottomOf="@+id/textView130" />

    <Button
        android:id="@+id/button99"
        android:layout_width="150dp"
        android:layout_height="110dp"
        android:layout_marginTop="30dp"
        android:onClick="goto_to_main"
        android:text="처음화면"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/view7"
        app:layout_constraintTop_toBottomOf="@+id/view7" />

    <Button
        android:id="@+id/button100"
        android:layout_width="150dp"
        android:layout_height="110dp"
        android:layout_marginStart="30dp"
        android:onClick="goto_fee"
        android:text="이전화면"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/button99"
        app:layout_constraintTop_toTopOf="@+id/button99" />

    <TextView
        android:id="@+id/textView153"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:gravity="center"
        android:text="부"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/textView130"
        app:layout_constraintTop_toTopOf="@+id/textView130" />
</androidx.constraintlayout.widget.ConstraintLayout>